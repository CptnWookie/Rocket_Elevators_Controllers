DEFINE battery USING columnAmount AND floorAmount AND floorGroup
    columnList: empty List
    status: Online

    SEQUENCE createColumnList USING columnAmount
        INIT iterator TO 1
        FOR columnAmount
            INIT newColumn TO INSTANTIATE column WITH iterator AND Online
            ADD newColumn TO columnList
            INCREMENT iterator 
        ENDFOR
    ENDSEQUENCE
ENDDEFINE



DEFINE column USING id AND floorAmount AND _minimumFloor AND _maximumFloor
    ID: id,
    status: Online
    floorAmount: _floorAmount
    minimumFloor: _minimumFloor
    maximumFloor: _maximumFloor
    elevatorList: empty LIST
ENDDEFINE





