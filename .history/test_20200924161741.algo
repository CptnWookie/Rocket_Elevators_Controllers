DEFINE battery USING _amountOfColumn
    columnList: empty List
    status: Online

    SEQUENCE createColumnList USING _amountOfColumn
        INIT iterator TO 1
        FOR _amountOfColumn
            INIT newColumn TO INSTANTIATE column WITH iterator AND Online
            ADD newColumn TO columnList
            INCREMENT iterator 
        ENDFOR
    ENDSEQUENCE
ENDDEFINE



DEFINE column USING id AND _floorAmount AND _minimumFloor AND _maximumFloor
    ID: id,
    status: Online
    floorAmount: _floorAmount
    minimumFloor:
    maximumFloor:
    elevatorList:
ENDDEFINE





